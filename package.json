{
  "name": "openserver-cli",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sh bin/preload.sh && vue-cli-service electron:serve",
    "build": "sh bin/preload.sh && vue-cli-service electron:build",
    "build:win": "sh bin/preload.sh && vue-cli-service electron:build --win",
    "web:serve": "sh bin/preload.sh && vue-cli-service serve",
    "web:build": "sh bin/preload.sh && vue-cli-service build",
    "web:lint": "sh bin/preload.sh && vue-cli-service lint",
    "postinstall": "sh bin/preload.sh && electron-builder install-app-deps",
    "postuninstall": "sh bin/preload.sh && electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "fs": "^0.0.1-security",
    "node-docker-api": "^1.1.22",
    "path": "^0.12.7",
    "vue": "^3.0.0",
    "vue-router": "^4.0.12",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
